@using RangeManagementSystem.Web.Models
@model WeaponsViewModel

@{
}
<h1>CliendDashboard</h1>
<table class="table">
    <thead>
        <tr>
            <th>Caliber</th>
            <th>Type</th>
            <th>Quantity</th>
            <th>Availability</th>
            <!-- Add more headers if needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Weapons)
        {
            <tr>
                <td>@item.Caliber</td>
                <td>@item.Type</td>
                <td>@item.Quantity</td>
                <td>@item.Availability</td>
                <!-- Add more cells if needed -->
            </tr>
        }
    </tbody>
</table>